<div id="<%= dom_id user %>">
  <p>
    <strong>Given name:</strong>
    <%= user.given_name %>
  </p>

  <p>
    <strong>Family name:</strong>
    <%= user.family_name %>
  </p>

  <p>
    <strong>Email:</strong>
    <%= user.email %>
  </p>

  <p>
    <strong>Preferred timezone:</strong>
    <%= user.preferred_timezone %>
  </p>

</div>
